// set focus to each field with a mask, to apply default mask to initial value
if( document.getElementById )
{
    document.getElementById('date_of_birth').focus();
    document.getElementById('program_end_date').focus();
    document.getElementById('family_member_1_date_of_birth').focus();
    document.getElementById('family_member_2_date_of_birth').focus();
    document.getElementById('family_member_3_date_of_birth').focus();
    document.getElementById('family_member_4_date_of_birth').focus();
    // set focus to first field in the form
    if( document.getElementById('first_name').type == "text" )
    {
        document.getElementById('first_name').focus();
    }
}
if( document.all )
{
    document.all['date_of_birth'].focus();
    document.all['program_end_date'].focus();
    document.all['family_member_1_date_of_birth'].focus();
    document.all['family_member_2_date_of_birth'].focus();
    document.all['family_member_3_date_of_birth'].focus();
    document.all['family_member_4_date_of_birth'].focus();
    // set focus to first field in the form
    if( document.all('first_name').type == "text" )
    {
        document.all['first_name'].focus();
    }
}
